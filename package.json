{
    "name": "@dimanoid/ngx-dm-dialog",
    "version": "0.2.18",
    "scripts": {
        "build": "ng build lib",
        "build:demo": "ng build --aot --prod --base-href /ngx-dm-dialog/ demo-app",
        "test": "jest --verbose projects/lib",
        "test:watch": "jest projects/lib --watch --notify",
        "cover": "jest projects/lib --coverage",
        "coverall": "jest projects/lib --coverage && cat ./coverage/lcov.info | coveralls",
        "lint": "ng lint",
        "publish": "npm publish dist/lib --access public",
        "up": "cd projects/lib && npm version patch && git add package.json && git commit -m \"version bump [ci skip]\" package.json && cd ../.. && npm version patch",
        "up:minor": "cd projects/lib && npm version minor && git add package.json && git commit -m \"version bump [ci skip]\" package.json && cd ../.. && npm version minor"
    },
    "dependencies": {
        "@angular/animations": "~11.2.12",
        "@angular/common": "~11.2.12",
        "@angular/compiler": "~11.2.12",
        "@angular/core": "~11.2.12",
        "@angular/forms": "~11.2.12",
        "@angular/platform-browser": "~11.2.12",
        "@angular/platform-browser-dynamic": "~11.2.12",
        "@angular/router": "~11.2.12",
        "rxjs": "~7.0.0",
        "tslib": "^2.2.0",
        "zone.js": "~0.11.4",
        "ng-zorro-antd": "^11.4.1"
    },
    "devDependencies": {
        "@angular-builders/jest": "^11.2.0",
        "@angular-devkit/build-angular": "~0.1102.11",
        "@angular-devkit/build-ng-packagr": "~0.1002.0",
        "@angular/cli": "~11.2.11",
        "@angular/compiler-cli": "~11.2.12",
        "@angular/language-service": "~11.2.12",
        "@ngneat/spectator": "~7.1.0",
        "@types/jest": "^26.0.23",
        "@types/node": "~15.0.2",
        "codelyzer": "^6.0.2",
        "jest": "^26.6.3",
        "jest-preset-angular": "^8.4.0",
        "ng-packagr": "^11.2.4",
        "resize-observer-polyfill": "^1.5.1",
        "ts-node": "~9.1.1",
        "tsickle": "^0.39.1",
        "tslint": "~5.20.1",
        "typescript": "~4.1.5"
    }
}
